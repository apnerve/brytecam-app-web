{
  "name": "brytecam-app-web",
  "version": "0.0.1",
  "description": "A web app built using Brytecam",
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "build": "webpack --mode=production --config webpack.config.js --env.production",
    "electron-build": "electron-builder",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open --https --cert ./server.cert --key  ./server.key",
    "electron-pack": "electron-builder -c.extraMetadata.main=public/electron.js",
    "electron-pack-mac": "electron-builder --mac -c.extraMetadata.main=public/electron.js"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      "public/electron.js"
    ],
    "extends": null,
    "directories": {
      "output": "packaged",
      "buildResources": "assets"
    }
  },
  "author": "brytecam",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/runtime": "7.10.2",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.5.3",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "node-sass": "4.14.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "antd": "^3.26.16",
    "brytecam-sdk-js": "^0.1.0",
    "copy-webpack-plugin": "^5.0.5",
    "electron-is-dev": "^1.2.0",
    "mdi-react": "^6.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-mdi": "^0.5.7",
    "react-router": "^5.0.1",
    "reactjs-localstorage": "1.0.1"
  },
  "keywords": [
    "brytecam",
    "sfu",
    "webrtc",
    "js"
  ]
}
